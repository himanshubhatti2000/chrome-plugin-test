{
    "manifest_version": 2,
    "name": "Button Injection Extension",
    "version": "1.0",
    "description": "Adds a test button to web pages.",
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_title": "Inject Button"
    }
  }
  